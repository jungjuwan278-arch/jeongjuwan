[
{
"key": "ctrl+alt+c",
"command": "workbench.action.tasks.runTask",
"args": "Compile C",
"when": "resourceExtname == .c"
},
{
"key": "ctrl+alt+c",
"command": "workbench.action.tasks.runTask",
"args": "Compile C++",
"when": "resourceExtname == .cpp"
},
{
"key": "ctrl+alt+r",
"command": "workbench.action.tasks.runTask",
"args": "Run Exe"
}
]
